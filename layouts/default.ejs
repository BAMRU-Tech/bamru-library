<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= title %></title>
    <!-- HTML5 shim, for IE6-8 support of HTML elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    <!-- styles -->
    <script src="https://***REMOVED***" async></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="/assets/scripts/main.js" charset="utf-8"></script>
    <link href="/assets/css/style.css" rel="stylesheet">
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
  </head>
  <body>
    <h1 class="visually-hidden">Default template</h1>
    <%- include partials/header %>
    <div class="g-body">
      <div class="g-left-panel">
        <% if (locals.sections && locals.sections.length) { %>
          <%- include partials/sectionList %>
        <% } %>
        <%- include partials/siblingList %>
      </div>

      <% if (locals.content) { %>
      <div class="g-main-content">
        <%- content %>
      </div>
      <% } %>

      <div class="g-footer">
        <button type="button" class="btn-footer"><a href="<%- editLink %>">Edit This Page</a></button>
        <button type="button" class="btn-footer"><a href="#">Create New Page</a></button>
        <button type="button" class="btn-plaintext"><a href="#">How do I use this site?</a></button>
      </div>
    </div>
    <script>
      // Google Analytics tracking
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', '***REMOVED***', 'auto');
      ga('send', 'pageview');
    </script>
  </body>
</html>
