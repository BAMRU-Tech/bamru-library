<p class="children-kicker">Get started by browsing these documents:</p>
<ul class="children">
  <% children.forEach((link) => { %>
    <li>
      <a class="content-link" href="<%- link.url %>" ><%= link.name %></a>
      <!-- <a class="edit-link" href="<%- link.editLink %>">(edit)</a> -->
    </li>
  <% }) %>
</ul>
